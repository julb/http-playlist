{
    "name": "http-url-playlist",
    "version": "1.0.0",
    "description": "A simple webapp which display sequentially the given URLs.",
    "main": "index.js",
    "scripts": {
        "build": "webpack",
        "format:check": "prettier --config ./.prettierrc --check \"src/**/*{.ts,.js,.json,.css,.scss,.less,.html}\" *.json",
        "format:fix": "prettier --config ./.prettierrc --write \"src/**/*{.ts,.js,.json,.css,.scss,.less,.html}\" *.json",
        "get-version": "echo $npm_package_version",
        "serve": "webpack serve",
        "test": "echo \"NOOP\""
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/julb/http-url-playlist.git"
    },
    "author": "julien@julb.me",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/julb/http-url-playlist/issues"
    },
    "homepage": "https://github.com/julb/http-url-playlist#readme",
    "devDependencies": {
        "css-loader": "^5.2.4",
        "css-minimizer-webpack-plugin": "^2.0.0",
        "file-loader": "^6.2.0",
        "html-webpack-plugin": "^5.3.1",
        "image-webpack-loader": "^7.0.1",
        "mini-css-extract-plugin": "^1.6.0",
        "prettier": "^2.2.1",
        "terser-webpack-plugin": "^5.1.1",
        "url-loader": "^4.1.1",
        "webpack": "^5.36.1",
        "webpack-cli": "^4.6.0",
        "webpack-dev-server": "^3.11.2"
    },
    "dependencies": {
        "jquery": "^3.6.0"
    }
